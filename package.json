{
  "name": "qrgrid",
  "description": "javascript qr generator",
  "type": "module",
  "private": false,
  "version": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yadav-saurabh/qrgrid.git"
  },
  "keywords": [
    "qr",
    "nodejs",
    "typescript",
    "react",
    "angular",
    "vue"
  ],
  "author": "yadav-saurabh",
  "scripts": {
    "changeset": "changeset",
    "prune": "turbo prune",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish-packages": "turbo run build && changeset version && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "prettier": "^3.3.3",
    "turbo": "^2.4.2"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "npm@10.5.2",
  "workspaces": [
    "docs",
    "packages/*",
    "examples/*"
  ],
  "overrides": {
    "typescript": "~5.5.4"
  },
  "volta": {
    "node": "22.14.0"
  }
}
