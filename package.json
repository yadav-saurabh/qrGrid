{
  "name": "qrgrid",
  "description": "javascript qr generator",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yadav-saurabh/qrgrid"
  },
  "keywords": [
    "qr",
    "nodejs",
    "typescript",
    "react",
    "angular",
    "vue"
  ],
  "author": "yadav-saurabh",
  "scripts": {
    "changeset": "changeset",
    "prune": "turbo prune",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish-packages": "turbo run build && changeset version && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "prettier": "^3.3.3",
    "turbo": "^2.0.11",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "npm@10.5.2",
  "workspaces": [
    "docs",
    "packages/*",
    "examples/*"
  ],
  "volta": {
    "node": "20.13.1"
  }
}
