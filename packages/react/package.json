{
  "name": "@qrgrid/react",
  "version": "1.3.3",
  "type": "module",
  "homepage": "https://www.qrgrid.dev/",
  "module": "src/index.ts",
  "main": "src/index.ts",
  "private": false,
  "description": "The Ultimate Customizable QR Code JavaScript Library QR Grid for react",
  "license": "MIT",
  "bugs": "https://github.com/yadav-saurabh/qrgrid/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/yadav-saurabh/qrgrid",
    "directory": "packages/react"
  },
  "keywords": [
    "qr",
    "qrgrid",
    "styling",
    "react"
  ],
  "files": [
    "/dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./canvas": "./src/canvas/index.ts",
    "./svg": "./src/svg/index.ts"
  },
  "scripts": {
    "build": "npm run clean && rollup -c && npm run cp:license && npm run mod:pkg",
    "clean": "rimraf ./dist",
    "cp:license": "cp ../../LICENSE ./LICENSE",
    "mod:pkg": "node ./scripts/modify-package.mjs",
    "type-check": "tsc --noEmit",
    "tsc:build": "tsc -b",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@qrgrid/core": "*"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.14.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "rimraf": "^6.0.1",
    "rollup": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
